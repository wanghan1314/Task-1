.news-container {
  width: 100%;
  height: 100vh;
}
.news-container .title {
  display: flex;
  justify-content: center;
  margin-bottom: 20rpx
}
.news-container .list {
  display: block;
  width: 100%;
  padding: 10rpx 0;
  border-top: 1px solid #edece8
}
.news-container .btn {
  display: flex;
  justify-content: center;
  margin-top: 100rpx
}
button {
  display: flex;
  justify-content: center;
  align-items: center;
   width: 500rpx;
  height: 100rpx;
  background-color: #00b26a;
 
}