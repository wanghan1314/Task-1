<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mutesaid.mapper.ExpertMapper">
    <resultMap id="BaseResultMap" type="com.mutesaid.model.Expert">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="position" jdbcType="VARCHAR" property="position"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="icon" jdbcType="VARCHAR" property="icon"/>
        <result column="intro" jdbcType="VARCHAR" property="intro"/>
        <result column="salary" jdbcType="INTEGER" property="salary"/>
        <result column="create_at" jdbcType="BIGINT" property="createAt"/>
        <result column="update_at" jdbcType="BIGINT" property="updateAt"/>
    </resultMap>

    <parameterMap id="Expert" type="com.mutesaid.model.Expert"/>

    <sql id="Base_Column_List">
        id, position, name, icon, intro, salary, create_at, update_at
    </sql>

    <select id="findList" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from expert
    </select>
</mapper>