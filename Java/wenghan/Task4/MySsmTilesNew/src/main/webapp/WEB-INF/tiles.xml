        <put-attribute name="tail" value="/WEB-INF/jsp/T0tail.jsp"/>
        <put-attribute name="script" value="/WEB-INF/jsp/T0script.jsp"/>
    </definition>
    <!--T1页-->
    <definition name="T1Layout" extends="Layout">
        <put-attribute name="head" value="/WEB-INF/jsp/T1head.jsp"/>
        <put-attribute name="title" value="无标题文档"/>
        <put-attribute name="body1" value="/WEB-INF/jsp/T1body1.jsp"/>
