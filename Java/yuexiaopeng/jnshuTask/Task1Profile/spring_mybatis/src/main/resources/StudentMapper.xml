<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatisPool.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xiuzhenyuan.dao.StudentDAO">

    <!--按照study_id删除数据-->
    <delete id="doDelete" parameterType="String" >
		delete from student where study_id = #{study_id}
    </delete>

    <!--插入数据-->
    <insert id="doInsert" parameterType="xiuzhenyuan.bean.Student" >
		insert into student values(#{id},#{name},#{create_at},#{update_at},#{qq},#{study_type},#{join_time},#{university},#{study_id},#{daily_link},#{slogen},#{master} )
    </insert>

    <!--修改数据，根据study_id-->
    <update id="doUpdate" parameterType="xiuzhenyuan.bean.Student" >
		update student set slogen= #{slogen} where study_id=#{study_id}
    </update>

    <!--查询数据，根据study_id-->
    <select id="doSelectByStudy_id" parameterType="string" resultType="xiuzhenyuan.bean.Student">
		select * from student  where study_id=#{study_id}
    </select>

    <!--查询数据，全部数据，用list存储-->
    <select id="doSelectAll" parameterType="string" resultType="xiuzhenyuan.bean.Student">
		select * from student
    </select>
</mapper>