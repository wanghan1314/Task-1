<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jnshu.MyBatis.ProfessionDao">
    <select id="all" resultType="com.jnshu.model.Profession">
        select * from profession where direction=1
    </select>
    <select id="after" resultType="com.jnshu.model.Profession">
        select *from  profession  where direction=2
    </select>
    <select id="ops" resultType="com.jnshu.model.Profession">
         select *from  profession  where direction=3
    </select>
</mapper>